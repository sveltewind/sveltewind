{
  "name": "sveltewind",
  "version": "1.6.0",
  "description": "Svelte UI framework focusing on Tailwindcss integration",
  "main": "index.js",
  "module": "index.js",
  "svelte": "index.js",
  "scripts": {
    "commit": "git add . && cz && npm run standard-version",
    "standard-version": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sveltewind/sveltewind.git"
  },
  "keywords": [
    "svelte",
    "sveltejs",
    "ui",
    "framework",
    "tailwind",
    "tailwindcss",
    "sveltewind"
  ],
  "author": "Bob McAleavey <bobmcaleavey@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sveltewind/sveltewind/issues"
  },
  "homepage": "https://github.com/sveltewind/sveltewind#readme",
  "dependencies": {
    "@steeze-ui/heroicons": "^1.1.1",
    "@steeze-ui/svelte-icon": "^1.3.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "svelte": "^3.46.4",
    "tailwind-merge": "^1.2.0"
  },
  "type": "module",
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "standard-version": "^9.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "skip": {
      "bump": false,
      "commit": true,
      "tag": true
    }
  }
}

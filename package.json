{
	"name": "sveltewind",
	"version": "3.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"npm-publish": "git push --follow-tags origin main && npm publish",
		"release": "commit-and-tag-version && npm run npm-publish"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./actions": {
			"types": "./dist/actions/index.d.ts",
			"svelte": "./dist/actions/index.js"
		},
		"./components": {
			"types": "./dist/components/index.d.ts",
			"svelte": "./dist/components/index.js"
		},
		"./icons": {
			"types": "./dist/icons/index.d.ts",
			"svelte": "./dist/icons/index.js"
		},
		"./themes": {
			"types": "./dist/themes/index.d.ts",
			"svelte": "./dist/themes/index.js"
		},
		"./utilities": {
			"types": "./dist/utilities/index.d.ts",
			"svelte": "./dist/utilities/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/eslint": "^9.6.0",
		"@types/luxon": "^3.4.2",
		"@types/node": "^22.9.0",
		"autoprefixer": "^10.4.20",
		"commit-and-tag-version": "^12.5.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"lucide-svelte": "^0.407.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.9",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.0.3"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"keywords": [
		"svelte",
		"sveltejs",
		"ui",
		"framework",
		"tailwind",
		"tailwindcss",
		"sveltewind",
		"library",
		"ui library",
		"sveltekit"
	],
	"license": "MIT",
	"dependencies": {
		"imask": "^7.6.1",
		"luxon": "^3.5.0",
		"shiki": "^1.23.0",
		"svelte-portal": "^2.2.1",
		"tailwind-merge": "^2.5.4"
	}
}

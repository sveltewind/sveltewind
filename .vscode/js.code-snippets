{
	"Add Component": {
		"prefix": "@add to index.js",
		"body": ["export { default as $1 } from './$1/index.js'"]
	},
	"Component index.js": {
		"prefix": "@create index.js",
		"body": ["export { default } from './index.svelte';"]
	},
	"Better import": {
		"prefix": "@import",
		"body": ["import { $2 } from '$1';"]
	}
}

{
  "Initiate Component": {
    "prefix": "@component",
    "body": [
      "<script>",
      "\t// imports",
      "\t// import {  } from '../../components';",
      "\timport { current_component } from 'svelte/internal';",
      "\timport { twMerge } from 'tailwind-merge';",
      "\timport { getEvents, use as useAction } from '../../actions';",
      "\timport { theme } from '../../stores';",
      "\t",
      "\t// handlers",
      "\t",
      "\t// helpers",
      "\t",
      "\t// props (internal)",
      "\tconst events = getEvents(current_component);",
      "\t",
      "\t// props (external)",
      "\texport let style = undefined;",
      "\texport let use = [];",
      "\t$2",
      "\t// props (dynamic)",
      "\t\\$: classes = twMerge('', \\$theme.${1:div}, \\$\\$props.class);",
      "</script>",
      "",
      "<${1:div} class={classes} use:useAction={[events, ...use]}>",
      "\t<slot/>",
      "</${1:div}>"
    ]
  },
  "Initiate Route": {
    "prefix": "@route",
    "body": [
      "<script>",

      "\t// handlers",
      "\t",
      "\t// helpers",
      "\t",
      "\t// props (internal)",
      "\t",
      "\t// props (external)",
      "\t",
      "\t// props (dynamic)",
      "\t",
      "\t// lifecycle",
      "\tonMount(async() => {",
      "\t\t\\$routeChange = false;",
      "\t});",
      "</script>",
      "",
